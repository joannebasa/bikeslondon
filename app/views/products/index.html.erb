<section id="products">
  <h1>Bikes for Sale</h1>
  <%= link_to 'New Product', new_product_path %>
  <div class="container-fluid">
    <div class="row">
      <% @products.each do |product| %>
      <%= render partial: "shared/product", locals: {product: product} %>
      <% end %> 
    </div><!-- end row -->
  </div><!-- end container-fluid -->
</section>